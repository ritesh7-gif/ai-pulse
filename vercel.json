{
    "version": 2,
    "builds": [
        {
            "src": "server.ts",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}